# Geen definitie in NEN3610
:applicatieschema a skos:Concept;
  rdfs:label "Applicatieschema"@nl, "Application schema"@en;
  dct:source "NEN3610:2020 Basismodel Geo-informatie - Sectie 3"@nl, "NEN-EN-ISO 19101-1:2014 Geographic information - Reference model - Part 1: Fundamentals "@en;
  skos:definition "Informatiemodel dat gegevens beschrijft die worden gebruikt door een of meer applicaties"@nl;
  skos:exactMatch <https://isotc211.geolexica.org/concepts/15>;
  skos:inScheme <https://begrippen.geostandaarden.nl/nen3610-2022>;
  skos:prefLabel "Applicatieschema"@nl;
  skos:related :conceptueel_informatiemodel, :logisch_informatiemodel;
  skos:scopeNote "Dit Basismodel geo-informatie is met UML beschreven in een applicatieschema."@nl;
  skos:topConceptOf <https://begrippen.geostandaarden.nl/nen3610-2022> .

:werkelijkheid a skos:Concept;
  rdfs:label "Werkelijkheid"@nl, "Universe of discourse"@en;
  dct:source "NEN3610:2020 Basismodel Geo-informatie - Sectie 3"@nl, "NEN-EN-ISO 19101-1:2014 Geographic information - Reference model - Part 1: Fundamentals"@en;
  skos:altLabel "Werkelijkheid"@nl;
  skos:definition "Beeld van de echte of hypothetische wereld dat binnen de context van een domein alles van belang omvat."@nl;
  skos:exactMatch <https://isotc211.geolexica.org/concepts/486>;
  skos:inScheme <https://begrippen.geostandaarden.nl/nen3610-2022>;
  skos:prefLabel "Beschouwingsgebied"@nl;
  skos:topConceptOf <https://begrippen.geostandaarden.nl/nen3610-2022> .

:domein_package a skos:Concept;
  dct:source "NEN3610:2020 Basismodel Geo-informatie - Sectie 7"@nl;
  skos:definition "Een unieke verwijzing naar een registratie van objecten."@nl;
  skos:inScheme <https://begrippen.geostandaarden.nl/nen3610-2022>;
  skos:prefLabel "Domein (package)"@nl;
  skos:topConceptOf <https://begrippen.geostandaarden.nl/nen3610-2022> .

:geen_waarde a skos:Concept;
  rdfs:label "Geen waarde"@nl;
  dct:source "NEN3610:2020 Basismodel Geo-informatie - Sectie 7"@nl;
  skos:definition "Element heeft in werkelijkheid geen waarde"@nl;
  skos:inScheme <https://begrippen.geostandaarden.nl/nen3610-2022>;
  skos:prefLabel "Geen waarde"@nl .

# :identificatie a skos:Concept;
