  skos:definition "Collectie van datasets met dezelfde specificaties."@nl;
  skos:exactMatch iso:125;
  skos:inScheme bk:nen3610-2022;
  skos:prefLabel "Datasetserie"@nl;
  skos:related :dataset;
  skos:topConceptOf bk:nen3610-2022 .

