  skos:prefLabel "Coördinaatsysteem"@nl;
  skos:related :coordinaatreferentiesysteem, :datum;
  skos:topConceptOf bk:nen3610-2022 .

:data a skos:Concept;
  rdfs:label "Data"@nl, "Data"@en;
