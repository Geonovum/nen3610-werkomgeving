  mim:herkomstDefinitie "nen 3610";
  mim:datumOpname "20220601";
  rdfs:label "Scheepvaartruimte";
  mim:toelichting "Scheepvaartruimte wordt gebruikt door allerlei soorten schepen.";
  rdfs:comment "Verkeerruimte voor voertuigen die zich over water verplaatsen.";
  mim:herkomst "nen 3610";
